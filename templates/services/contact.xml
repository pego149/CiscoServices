    <MenuItem>
        <Name>Cele meno a priezvisko</Name>
        <URL>http://{{ request.get_host }}/CiscoServices/message/{{ contact.name }}</URL>
    </MenuItem>
    <MenuItem>
        <Name>Pracovisko</Name>
        <URL>http://{{ request.get_host }}/CiscoServices/message/{{ contact.job }}</URL>
